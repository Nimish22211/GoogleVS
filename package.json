{
	"name": "googlevs",
	"displayName": "",
	"publisher": "Nimish",
	"description": "its a extenstion to use google search inside vs code. Now you won't have to move out of VScode.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.65.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:googlevs-sidebar",
		"onCommand:googlevs.google"
	],
	"main": "./extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "googlevs-sidebar-view",
					"title": "Google VS",
					"icon": "media/globe.svg"
				}
			]
		},
		"views": {
			"googlevs-sidebar-view": [
				{
					"type": "webview",
					"id": "googlevs-sidebar",
					"name": "Google VS",
					"icon": "media/globe.svg",
					"contextualTitle": "Google VS"
				}
			]
		},
		"commands": [
			{
				"command": "googlevs.google",
				"title": "google",
				"category": "GoogleVS"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.65.0",
		"@vscode/test-electron": "^2.1.2",
		"eslint": "^8.9.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5"
	},
	"dependencies": {
		"node-fetch": "^3.2.3"
	}
}
